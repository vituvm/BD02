<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Compra.
    -->
    <changeSet id="20220421184214-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="curso_id"
                                 baseTableName="compra"
                                 constraintName="fk_compra__curso_id"
                                 referencedColumnNames="id"
                                 referencedTableName="curso"/>

        <addForeignKeyConstraint baseColumnNames="usuario_id"
                                 baseTableName="compra"
                                 constraintName="fk_compra__usuario_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario"/>
    </changeSet>
</databaseChangeLog>
